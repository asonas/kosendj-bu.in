(function() {
  $(function() {
    $(".js-popup").click(function() {
      var $panel, name;
      $(".panel").addClass("hidden");
      name = $(this).parents(".icon").data("name");
      $panel = $(".js-" + name);
      $panel.removeClass("hidden");
      return $(".rug").removeClass("hidden");
    });
    return $(".js-close, .rug").click(function() {
      $(".panel").addClass("hidden");
      return $(".rug").addClass("hidden");
    });
  });

}).call(this);
